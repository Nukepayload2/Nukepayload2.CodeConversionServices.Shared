Friend Module Brushes
    Public Function GetSolidBrushFromARGB(ARGB As Integer) As SolidColorBrush
        Dim argbv = BitConverter.GetBytes(ARGB)
        Return New SolidColorBrush(New Color With {.A = argbv(3), .R = argbv(2), .G = argbv(1), .B = argbv(0)})
    End Function
